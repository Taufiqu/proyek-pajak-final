[flake8]
max-line-length = 88
extend-ignore = E203, W503, E501
exclude = 
    .git,
    __pycache__,
    venv,
    migrations,
    build,
    dist,
    .eggs,
    *.egg-info
per-file-ignores = 
    __init__.py:F401
    test_*.py:S101
max-complexity = 10
import-order-style = google
application-import-names = app, models, config, faktur, bukti_setor, shared_utils
